<p id="notice"><%= notice %></p>

<h1>Scrollzs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrollzs.each do |scrollz| %>
      <tr>
        <td><%= link_to 'Show', scrollz %></td>
        <td><%= link_to 'Edit', edit_scrollz_path(scrollz) %></td>
        <td><%= link_to 'Destroy', scrollz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scrollz', new_scrollz_path %>
